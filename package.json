{
  "name": "react-redux-shopping-list",
  "version": "1.0.0",
  "description": "Full Stack application using React, Redux and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "yarn install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn start --prefix client",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "keywords": [
    "full-stack",
    "react",
    "redux",
    "express",
    "shopping-list"
  ],
  "author": "Manuel F. Del Toro",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "config": "^3.0.1",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.19"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
